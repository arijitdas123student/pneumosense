<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="scanQuestionOneContainerVM" type="com.arijitsapplication.app.modules.scanquestiononecontainer.data.viewmodel.ScanQuestionOneContainerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearStackarrowleft"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_6pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/fragmentContainer"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylewhite_6">
            <LinearLayout android:id="@+id/linearRowsend"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_35pxv"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnlightbulb"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <LinearLayout android:id="@+id/linearRowicon"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
                        <LinearLayout
                            android:id="@+id/linearRowlightbulb"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_weight="0.88"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLightbulb"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_22pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_22pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_lightbulb"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtTypeamessage"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_2pxh"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_weight="0.82"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{scanQuestionOneContainerVM.scanQuestionOneContainerModel.txtTypeamessage}"
                                style="@style/txtInterregular12_2" tools:text="@string/msg_type_a_message"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_114pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameButtonIcon"
                    android:layout_height="@dimen/_48pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_48pxh" style="@style/groupStyleindigo_2">
                    <ImageView android:id="@+id/imageSend"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_send"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_send"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
