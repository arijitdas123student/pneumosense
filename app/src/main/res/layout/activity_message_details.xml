<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="messageDetailsVM" type="com.arijitsapplication.app.modules.messagedetails.data.viewmodel.MessageDetailsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearNavbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDrNSingh"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_103pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.77"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{messageDetailsVM.messageDetailsModel.txtDrNSingh}"
                    style="@style/txtInterbold18" tools:text="@string/lbl_dr_n_singh"/>
                <ImageView android:id="@+id/imageCall"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_98pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_call"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameMessageDetails"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <View android:background="@color/gray_100"
                android:id="@+id/viewBackground"
                android:layout_gravity="start"
                android:layout_height="@dimen/_764pxh" android:layout_width="@dimen/_375pxh"/>
            <LinearLayout android:id="@+id/linearColumntime"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_88pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_88pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumndescription"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylewhite_7">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDescription"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{messageDetailsVM.messageDetailsModel.txtDescription}"
                        style="@style/txtInterregular12_3" tools:text="@string/msg_you_should_rest"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_117pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{messageDetailsVM.messageDetailsModel.txtTime}"
                    style="@style/txtMulishsemibold12" tools:text="@string/lbl_12_10"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumndescriptionOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStyleindigo_A200cornerRadius">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDescriptionOne"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{messageDetailsVM.messageDetailsModel.txtDescriptionOne}"
                        style="@style/txtInterregular12" tools:text="@string/msg_you_should_rest"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeOne"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_117pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{messageDetailsVM.messageDetailsModel.txtTimeOne}"
                    style="@style/txtMulishsemibold12" tools:text="@string/lbl_12_10"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumndescriptionTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylewhite_7">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDescriptionTwo"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{messageDetailsVM.messageDetailsModel.txtDescriptionTwo}"
                        style="@style/txtInterregular12_3" tools:text="@string/msg_you_should_rest"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_117pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{messageDetailsVM.messageDetailsModel.txtTimeTwo}"
                    style="@style/txtMulishsemibold12" tools:text="@string/lbl_12_10"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumndescriptionThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStyleindigo_A200cornerRadius">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDescriptionThree"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{messageDetailsVM.messageDetailsModel.txtDescriptionThree}"
                        style="@style/txtInterregular12" tools:text="@string/msg_you_should_rest"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeThree"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_117pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{messageDetailsVM.messageDetailsModel.txtTimeThree}"
                    style="@style/txtMulishsemibold12" tools:text="@string/lbl_12_10"/>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylewhite_6">
            <LinearLayout android:id="@+id/linearRowsend"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_35pxv"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnlightbulb"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <LinearLayout android:id="@+id/linearRowicon"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
                        <LinearLayout
                            android:id="@+id/linearRowlightbulb"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_weight="0.88"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLightbulb"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_22pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_width="@dimen/_22pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_lightbulb"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtTypeamessage"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_2pxh"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_weight="0.82"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{messageDetailsVM.messageDetailsModel.txtTypeamessage}"
                                style="@style/txtInterregular12_2" tools:text="@string/msg_type_a_message"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_114pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameButtonIcon"
                    android:layout_height="@dimen/_48pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_48pxh" style="@style/groupStyleindigo_2">
                    <ImageView android:id="@+id/imageSend"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_send"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_send"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
